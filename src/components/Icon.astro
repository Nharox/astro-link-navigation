---
import type { IconName } from '../models/icon-name';

import BlueskyIcon from '../icons/BlueskyIcon.astro';
import CodeIcon from '../icons/CodeIcon.astro';
import CopyrightIcon from '../icons/CopyrightIcon.astro';
import JoystickIcon from '../icons/JoystickIcon.astro';
import MastodonIcon from '../icons/MastodonIcon.astro';
import MusicIcon from '../icons/MusicIcon.astro';
import PhotoIcon from '../icons/PhotoIcon.astro';

interface Props {
  name: IconName;
}

const { name } = Astro.props;
---

{name === 'bluesky' && <BlueskyIcon />}
{name === 'code' && <CodeIcon />}
{name === 'copyright' && <CopyrightIcon />}
{name === 'joystick' && <JoystickIcon />}
{name === 'mastodon' && <MastodonIcon />}
{name === 'music' && <MusicIcon />}
{name === 'photo' && <PhotoIcon />}
